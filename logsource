logsource:
    category: registry_event
    product: windows

   detection:
    selection:
        target_object|regularexpression: CtM*
                                'Foxconn.lnk'
        details|containsall:
            - 'explorer.exe'
            - 'Intel'
    condition: selection
      
detection:
   selection1:
       image|endswith: schtask.exe
       commandline|containsall:
              - /create
              - /tn
              - Intel
              - WSscript.exe
        registry_add|endswith:
              - Foxconn.lnk
        condition:selection














logsource:
    product: windows
    category: process_creation
