logsource:
    product: windows
    category: network_connection

logsource:
    product: windows
    category: process_creation
detection:
    selection1:
        image: chrome.exe
        commandline: http://gmail.com

logsource:
    category: process_creation
    product: windows
detection:
    selection1:
        image: notepad.exe
        commandline: "echo <name>"

logsource:
    category: process_creation
    product: windows
detection:
    selection1:
        image: 'cmd.exe'
        commandline: 'shutdown /s'
    selection2:
        image: 'powershell.exe'
        commandline: 'Stop-Computer'
condition: selection1 or selection2







logsource:
    product:windows
    category:file_event
selection1:
    images|endswith: "onedrive.exe"
    targetfilename|endswith: ".pdf"
    targetfilename|endswith: "c:\"

logsource:
     product:windows
     category:process_creation
    detection:
    selection1:
        image|endswith: 'C:\Windows\System32\schtasks.exe'
    
    selection2:
        image: 'C:\Windows\System32\schtasks.exe'
        commandline|contains: '/create'

    selection3:
        image: 'C:\Windows\System32\schtasks.exe'
        commandline|contains: '/tn "Intel"'

    selection4:
        image: 'C:\Windows\System32\Wscript.exe'
 
    selection5:
        commandline|contains: 'Intel58d2a83f778d5.36783181.vbs'
    
    selection6:
        commandline|contains:
            - '/sc minute'
            - '/mo 25'
    condition: selection1 and selection2 and selection3 and selection4 and selection5 and selection6



detection:
    selection:
        image: 'C:\Windows\System32\schtasks.exe'
        commandline|containsall:
            - '/create'
            - '/tn "Intel"'
            - 'C:\Windows\System32\Wscript.exe'
            - 'Intel58d2a83f778d5.36783181.vbs'
            - '/sc minute'
            - '/mo 25'
    condition: selection


