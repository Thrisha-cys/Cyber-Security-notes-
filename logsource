logsource:
    product: windows
    category: process_creation
detection:
    selection1:
        Image: "wsmpovhost.exe"
        CommandLine|contains: "DsRole.dll"
    selection2:
        ParentImage: "wsmpovhost.exe"
        Image: "wksprt.exe"
    selection3:
        Image|endswith: "DsRole.dll"
        ParentImage|endswith: "wsmpovhost.exe"
condition: selection1 or selection2 or selection3



